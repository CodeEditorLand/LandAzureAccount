{
  "name": "azure-account-sample",
  "displayName": "Azure Account Sample",
  "license": "MIT",
  "repository": {
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git",
    "type": "git"
  },
  "version": "0.0.1",
  "publisher": "ms-vscode-sample",
  "engines": {
    "vscode": "1.58.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "azure"
  ],
  "activationEvents": [
    "onCommand:azure-account-sample.showSubscriptions",
    "onCommand:azure-account-sample.showAppServices"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "azure-account-sample.showSubscriptions",
        "title": "Show Subscriptions",
        "category": "Azure Example"
      },
      {
        "command": "azure-account-sample.showAppServices",
        "title": "Show AppServices",
        "category": "Azure Example"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "prepublishOnly": "TypeScriptESBuild Source/**/*.ts"
  },
  "extensionDependencies": [
    "ms-vscode.azure-account"
  ],
  "devDependencies": {
    "@microsoft/vscode-azext-utils": "2.1.3",
    "@types/node": "20.8.9",
    "@types/vscode": "1.83.1",
    "typescript": "5.2.2",
    "typescript-esbuild": "0.3.1"
  },
  "dependencies": {
    "@azure/arm-appservice": "14.0.0",
    "@azure/arm-resources": "5.2.0",
    "@azure/arm-subscriptions": "5.1.0"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "type": "module",
  "private": "false",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  }
}
